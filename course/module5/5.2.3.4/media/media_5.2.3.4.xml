<textcomponents>
	 <component id="ID_title">
        <text><![CDATA[<b>注册表键</b>]]></text> 
    </component>
	
	    <component id="ID_table01">
        <text><![CDATA[
        <table header='none' bordercolor='#FFFFFF' borderwidth='3' shadow='false' shadowcolor='#000000' glow='true' glowcolor='#808080' cellpadding='4'>
 
 			<tr class="mtable_title">
				<td colspan="2" bgcolor='#FFFFFF'><font size="16"><br/> </font></td>
			</tr>

            <tr class="mtable_top">
                <td colspan='2' bgcolor='#4E4C4D'><font size='1' color='#4E4C4D'>.</font></td>
            </tr>
 
            <tr class="mtable_title">
                <td bgcolor='#666263' width='150'><font color='#FFFFFF'><b>HKEY</b></font></td>
                <td bgcolor='#666263'><font color='#FFFFFF'><b>描述</b></font></td>
            </tr>

            <tr>
                <td bgcolor='#E9E7E8'>HKEY_CLASSES_ROOT</td>
                <td bgcolor='#E9E7E8'>有关将哪些文件扩展名映射到特定应用的信息</td>
            </tr>
			
			<tr>
                <td bgcolor='#F2F2F2'>HKEY_CURRENT_USER</td>
                <td bgcolor='#F2F2F2'>桌面设置和历史记录等与 PC 的当前用户相关的信息</td>
            </tr>
			
			<tr>
                <td bgcolor='#E9E7E8'>HKEY_USERS</td>
                <td bgcolor='#E9E7E8'>有关已登录系统的所有用户的信息</td>
            </tr>
			
			<tr>
                <td bgcolor='#F2F2F2'>HKEY_LOCAL_MACHINE</td>
                <td bgcolor='#F2F2F2'>与硬件和软件相关的信息</td>
            </tr>
			
			<tr>
                <td bgcolor='#E9E7E8'>HKEY_CURRENT_CONFIG</td>
                <td bgcolor='#E9E7E8'>与系统上所有活动设备相关的信息</td>
            </tr>
 
        
 
        </table>]]></text>
    </component>
</textcomponents>