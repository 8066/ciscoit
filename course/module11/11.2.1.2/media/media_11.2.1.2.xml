</component>

<component id='ID_table'>
<text><![CDATA[<table header='none' bordercolor='#FFFFFF' borderwidth='3' shadow='false' shadowcolor='#000000' glow='true' glowcolor='#808080' cellpadding='4' cellspacing='0'>
  <tr>
    <td colspan='2' bgcolor='#434321' class="liner_top"><font size='1' color='#434321'><span style="color:#DAE17A">.</span></font></td>
  </tr>
  <tr>
    <td bgcolor='#666263' valign="top"><font color='#ffffff'><b>PostScript</b></font></td>
    <td bgcolor='#666263' valign="top"><font color='#ffffff'><b>PCL</b></font></td>
  </tr>
  <tr>
    <td bgcolor='#E9E7E8' valign="top"><font color='#393536'>页面由打印机渲染</font></td>
    <td bgcolor='#E9E7E8' valign="top"><font color='#393536'>页面在本地工作站渲染</font></td>
  </tr>
    <tr>
    <td bgcolor='#F2F2F2' valign="top"><font color='#393536'>更优质的输出</font></td>
    <td bgcolor='#F2F2F2' valign="top"><font color='#393536'>更快地完成打印作业</font></td>
  </tr>
  <tr>
    <td bgcolor='#E9E7E8' valign="top"><font color='#393536'>处理更复杂的打印作业</font></td>
